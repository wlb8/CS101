import csv
from typing import Counter
def month_from_number(number):
    '''12 = December'''
    if number == '01':
        return "January"
    elif number == '02':
        return "February"
    elif number == '03':
        return "March"
    elif number == '04':
        return "April"
    elif number == '05':
        return "May"
    elif number == '06':
        return "June"
    elif number == '07':
        return "July"
    elif number == '08':
        return "August"
    elif number == '09':
        return "September"
    elif number == '10':
        return "October"
    elif number == '11':
        return "November"
    elif number == '12':
        return "December"


def read_in_file(filename):
    lines=[]
    f = open(filename,encoding='utf-8')
    fcsv = csv.reader(f)
    for line in fcsv:
        lines.append(line)
    return lines

def create_reported_date_dic(csvlist):
    crimedate={}
    dates=[]
    for x in csvlist:
        dates.append(csvlist[1])
    for date in dates:
        for crimedates in csvlist:
            if date == crimedates:
                crimedate[date]=1
    for time in crimedate:
        for x in dates:
            if time == x:
                crimedate[time] += 1

